
pipeline {
    agent {
        docker { 
          image 'yevhenii1991/myjenkins-slave:latest' 
        }
    }
    environment {
      DOCKERHUB_CREDENTIALS = credentials('jemkins-dockerhub')
    }
    stages {
    stage('Build') {
         steps { 
           sh 'docker ps -a'
	   sh 'docker images'
}
	 }
    stage('1234') {
         steps { 
	   sh 'docker build -t yevhenii1991/wordpress  https://github.com/yevheniistelmakh/lasttry/tree/main/uno'
		}
	 }
  }
}
