
pipeline {
    agent {
        docker { 
          image 'yevhenii1991/myjenkins-slave:latest' 
        }
    }
    environment {
      DOCKERHUB_CREDENTIALS = credentials('jemkins-dockerhub')
    }
    stage('Build') {
         steps { 
           sh 'docker ps -a'
	   sh 'docker images'
	   sh 'docker build -t yevhenii1991/wordpress'
		}
	 }
     
   
  }
}
