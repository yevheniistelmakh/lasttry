
pipeline {
    agent {
        docker { 
          image 'yevhenii1991/myjenkins-slave:latest' 
        }
    }
    environment {
      DOCKERHUB_CREDENTIALS = credentials('jemkins-dockerhub')
    }
  stages {
    stage("update jenkins job"){
      steps {
        script {
          echo "Test-00001"
        }
      }
    }
  }
  stage('Build') {
            steps { 
	         sh 'docker ps -a'
		}
	 }
  
}
