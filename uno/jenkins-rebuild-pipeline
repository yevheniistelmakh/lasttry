
pipeline {
    agent { label 'slave1'
    }
    
    triggers { pollSCM ('* * * * *')
    }
   
    stages {
      stage('variable value') {
        steps {
          sh 'export IMAGE_VERSION=latest'
	  sh 'echo $IMAGE_VERSION'
          dir('/home/digtal_ocean') {
          sh 'docker-compose down'
	  sh 'docker-compose up -d'
            }
        } 
 	}
  }
}
