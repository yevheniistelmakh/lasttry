
pipeline {
    agent { label 'slave1'
    }
    
    triggers { pollSCM ('* * * * *')
    }
   
    stages {
      stage('variable value') {
        steps {
          sh 'export ENV1=latest'
	  sh 'echo $ENV1'
          dir('/home/digtal_ocean') {
          sh "pwd"
	  sh 'docker-compose restart'
            }
        } 
 	}
  }
}
