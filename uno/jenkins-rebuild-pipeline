
pipeline {
    agent { label 'slave1'
    }
    
    triggers { pollSCM ('* * * * *')
    }
   
    stages {
      stage('variable value') {
        steps {
          sh 'export ENV1=latest'
	  sh 'echo $ENV1'
                
            }
        }
      stage("Env Variables") {
        steps {
          sh ""
            }
        }
      stage('') {
        steps { 
	  sh ''
		}
	     } 
  }
}
