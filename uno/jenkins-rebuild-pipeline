
pipeline {
    agent { label 'slave1'
    }
    
    triggers { pollSCM ('* * * * *')
    }
   
    stages {
      stage('variable value') {
        steps {
          sh 'export IMAGE_VERSION=latest'
	  sh 'echo $IMAGE_VERSION'
	  sh 'ls -al /home/digtal_ocean'
	  sh 'cd /home/digtal_ocean && docker-compose down && docker-compose up -d'
        } 
 	}
  }
}
